{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "plugins": ["node", "import"],
  "env": {
    "node": true,
    "jest": true
  },
  "settings": {
    "import/extensions": [".js", ".jsx"]
  },
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": { "order": "asc" },
        "groups": [["builtin", "external"], "sibling", "parent"]
      }
    ],
    "import/no-unresolved": [2, { "commonjs": true }]
  }
}
